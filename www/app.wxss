/**app.wxss**/
view,text{
  line-height: 1
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 状态栏------------通用 */
.status-bar{
  width: 100%;
  height: 130rpx;
  color: white;
}
.status-bar .return-icon{
  width: 20rpx;
  height: 30rpx;
  margin-right: 15rpx;
}
.status-bar .bg-img{
  width: 100%;
  height: 100%;
}
.return{
  position: absolute;
  left: 15rpx;
  top: 62rpx;
}


/* 弹性布局 */
.flex{
  display: flex;
  align-items: center;
  justify-content: space-between;
}